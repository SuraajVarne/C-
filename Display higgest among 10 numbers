// Accept 10 numbers display highest among 10 numbers

#include <iostream>
using namespace std;

int main()
{
    int counter = 0, large, num;

    while(counter < 10)
    {
        cout << "\n\t Enter large number: ";
        cin >> large;

        cout << "\n\t Enter number: ";
        cin >> num;

        if(large < num)
        {
            counter ++;
        }
        else 
        { 
            cout << "Enter num: ";
            cin >> num;
        }
         
         
    }


cout << large;

    return 0;
}
