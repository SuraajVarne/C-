// Fibonacci Series

#include <iostream>
using namespace std;

int main()
{
    int curr = 1, prev = 1, series, next;

    cout << "\n\t Enter series: ";
    cin >> series;

    cout << "\n\t Previous: " << prev;
    cout << "\n\t Current: " << curr;

    series = series = 2;

    if(series > 0)
    {
        next = prev + curr;
        cout << "\n\t Next: ";

        series = series - 1;
        prev = curr;
        curr = next;
    }

    return 0;
}
